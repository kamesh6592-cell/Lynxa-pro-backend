{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/keys/generate",
      "dest": "/api/generate-key.js"
    },
    {
      "src": "/api/lynxa",
      "dest": "/api/lynxa.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/info",
      "dest": "/api/info.js"
    },
    {
      "src": "/api/keys/revoke",  
      "dest": "/api/revoke-key.js"
    }
  ]
}
